<UserControl x:Class="IPlayer.Contracts.SettingsFlyLayout"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="768"
             d:DesignWidth="646"
             mc:Ignorable="d">

    <Grid Background="#FF7C7070">
        <Image x:Name="Image"
               Width="286"
               Height="280"
               Margin="350,10,0,0"
               HorizontalAlignment="Left"
               VerticalAlignment="Top" />
        <TextBlock x:Name="TextBlock1"
                   Width="315"
                 
                   Height="95"
                   Margin="10,10,0,0"
                   HorizontalAlignment="Left"
                   VerticalAlignment="Top"
                   FontSize="20"
                   Text="TextBlock"
                   TextAlignment="Justify"
                   TextWrapping="Wrap" />
        <TextBlock x:Name="DateCreated"
                   TextWrapping="Wrap"
                   Margin="10,120,321,603"
                   FontSize="20" />
        <TextBlock x:Name="Duration"
                   TextWrapping="Wrap"
                   Margin="10,205,321,518"
                   FontSize="20" />
        <TextBlock x:Name="Album"
                   TextWrapping="Wrap"
                   Margin="10,375,321,348"
                   FontSize="20" />
        <TextBlock x:Name="Size"
                   TextWrapping="Wrap"
                   Margin="10,295,321,428"
                   FontSize="20" />
        <TextBlock x:Name="Path"
                   TextWrapping="Wrap"
                   Margin="10,490,10,10"
                   FontSize="20" />
    </Grid>
</UserControl>

